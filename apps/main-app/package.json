  "type": "module",
  "exports": {
    "browser": "./src/client.tsx",
    "react-server": "./src/workers/rsc/index.tsx",
    "default": "./src/workers/main/index.ts"
  },
  "scripts": {
    "build": "webpack-rsc-edge-build",
    "build:dev": "webpack-rsc-edge-build --mode development",
    "deploy:main": "wrangler publish -c src/workers/main/wrangler.toml",
    "deploy:rsc": "wrangler publish -c src/workers/rsc/wrangler.toml",
    "dev:main": "npm run start:main",
    "dev:rsc": "npm run start:rsc",
    "start:main": "wrangler dev -c src/workers/main/wrangler.toml -l",
    "start:rsc": "wrangler dev -c src/workers/rsc/wrangler.toml -l",
    "watch": "webpack-rsc-edge-build --watch",
    "watch:dev": "webpack-rsc-edge-build --mode development --watch"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "countries-list": "^2.6.1",
    "cssnano": "^5.1.15",
    "fuse.js": "^6.6.2",
    "history": "^5.3.0",
    "htmlescape": "^1.1.1",
    "react": "18.3.0-next-f828bad38-20230313",
    "react-dom": "18.3.0-next-f828bad38-20230313",
    "react-markdown": "^8.0.5",
    "react-server-dom-webpack": "18.3.0-next-f828bad38-20230313",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.0",
